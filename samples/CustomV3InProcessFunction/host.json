{
    "version": "2.0",
    "logging": {
        "logLevel": {
            "default": "Information",
            "Azure.Messaging.ServiceBus": "Warning",
            "Azure.Core": "Error",
            "Microsoft.AspNetCore": "Warning",
            "Host.Controllers.Host": "Warning"
        },
        "applicationInsights": {
            "samplingSettings": {
                "isEnabled": true,
                "excludedTypes": "Request"
            }
        }
    }
}