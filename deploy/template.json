{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "type": "string",
            "defaultValue": "[resourceGroup().location]",
            "metadata": {
                "description": "Location for all resources."
            }
        },
        "resourceNamePrefix": {
            "type": "string",
            "metadata": {
                "description": "Used to create a unique name. For example, with a 'hello' prefix and an Application Insights resource, the resource name will be 'hello-appi'."
            }
        },
        "reallySecretValue": {
            "type": "securestring",
            "metadata": {
                "description": "Used to populate 'Secret:ReallySecretValue'."
            }
        }
    },
    "variables": {
        "applicationInsights": {
            "name": "[concat(parameters('resourceNamePrefix'), '-appi')]",
            "workspaceName": "[concat(parameters('resourceNamePrefix'), '-log')]"
        },
        "defaultV3InProcessFunctionApp": {
            "hostingPlanName": "[concat(parameters('resourceNamePrefix'), '-defaultv3inprocess-plan')]",
            "functionAppName": "[concat(parameters('resourceNamePrefix'), '-defaultv3inprocess-func')]",
            "storageName": "[concat(parameters('resourceNamePrefix'), '7defaultv3inprocess')]",
            "queue": "defaultv3inprocess-queue",
            "exceptionQueue": "defaultv3inprocess-exception-queue"
        },
        "defaultV4InProcessFunctionApp": {
            "hostingPlanName": "[concat(parameters('resourceNamePrefix'), '-defaultv4inprocess-plan')]",
            "functionAppName": "[concat(parameters('resourceNamePrefix'), '-defaultv4inprocess-func')]",
            "storageName": "[concat(parameters('resourceNamePrefix'), '7defaultv4inprocess')]",
            "queue": "defaultv4inprocess-queue",
            "exceptionQueue": "defaultv4inprocess-exception-queue"
        },
        "customV3InProcessFunctionApp": {
            "hostingPlanName": "[concat(parameters('resourceNamePrefix'), '-customv3inprocess-plan')]",
            "functionAppName": "[concat(parameters('resourceNamePrefix'), '-customv3inprocess-func')]",
            "storageName": "[concat(parameters('resourceNamePrefix'), '7customv3inprocess')]",
            "queue": "customv3inprocess-queue",
            "exceptionQueue": "customv3inprocess-exception-queue"
        },
        "customV4InProcessFunctionApp": {
            "hostingPlanName": "[concat(parameters('resourceNamePrefix'), '-customv4inprocess-plan')]",
            "functionAppName": "[concat(parameters('resourceNamePrefix'), '-customv4inprocess-func')]",
            "storageName": "[concat(parameters('resourceNamePrefix'), '7customv4inprocess')]",
            "queue": "customv4inprocess-queue",
            "exceptionQueue": "customv4inprocess-exception-queue"
        },
        "serviceBusNamespace": {
            "name": "[concat(parameters('resourceNamePrefix'), 'sb')]"
        }
    },
