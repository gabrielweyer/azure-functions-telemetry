            ],
            "properties": {
                "httpsOnly": true,
                "serverFarmId": "[resourceId('Microsoft.Web/serverfarms', variables('defaultV3InProcessFunctionApp').hostingPlanName)]"
            },
            "resources": [
                {
                    "type": "config",
                    "apiVersion": "2021-03-01",
                    "name": "appsettings",
                    "dependsOn": [
                        "[resourceId('Microsoft.Storage/storageAccounts', variables('defaultV3InProcessFunctionApp').storageName)]",
                        "[resourceId('Microsoft.Web/sites', variables('defaultV3InProcessFunctionApp').functionAppName)]",
                        "[resourceId('Microsoft.Insights/components', variables('applicationInsights').name)]",
