                        "ServiceBusConnection": "[listKeys(resourceId('Microsoft.ServiceBus/namespaces/authorizationRules', variables('serviceBusNamespace').name, 'RootManageSharedAccessKey'), '2017-04-01').primaryConnectionString]"
